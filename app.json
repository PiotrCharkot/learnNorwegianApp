{
  "expo": {
    "name": "LearnNorwegianApp",
    "slug": "LearnNorwegianApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo3.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo3NoBackground.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.piotrcharkot.LearnNorwegianApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo3.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "05ce5f16-e014-41bf-af82-3bb5e37d1c23"
      }
    }
  }
}
